face {\n  font-family: IPixRegular;\n  src: url(\"../font/IPixRegular.ttf\");\n  font-weight:800;\n}\n@font-face {\n  font-family: VonwaonBitmap12;\n  src: url(\"../font/VonwaonBitmap-12px.ttf\");\n  font-weight:1000;\n}\n@font-face {\n  font-family: VonwaonBitmap16;\n  src: url(\"../font/VonwaonBitmap-16px.ttf\");\n  font-weight:800;\n}\n#font-loader {\n  font-family: VonwaonBitmap16;\n  position: absolute;\n  left: -1000px;\n  visibility: hidden;\n}\n\nbody {\n  margin: 0;\n  padding: 0;\n  font-family: VonwaonBitmap16;\n  font-size: 40px;\n}\n#game-view {\n  background-image: url('../images/bg.png');\n  display: flex;\n  height: 100vh;\n  align-items: center;\n  justify-content: center;\n  color: $main-color;\n\n  @media screen and (max-width: 900px) {\n\n    canvas {\n      width: 100vw;\n    }\n  }\n  @import './dialogModal';\n  @import \"./welcome\";\n  @import './home';\n}\n\n\n","../public/style/variable.scss":"$main-color: #263238;\n$data-type: 'health', 'feeling', 'knowledge', 'relationship';","../public/style/dialogModal.scss":".dialogue {\n  @keyframes text-end {\n    0% {\n      content: '_';\n    }\n    100% {\n      content: '';\n    }\n  }\n  &.dialogue {\n    position: absolute !important;\n    bottom: 0;\n    height: 220px;\n    width: 90%;\n    border: 5px solid $main-color;\n    padding: 20px;\n    background-image: url('../images/bg.png');\n    box-shadow: inset 3px 3px rgba($main-color, 0.8);\n  }\n  #dialogue-text {\n    display: inline-block;\n    line-height: 55px;\n    &::after {\n      content: \"_\";\n      animation: text-end 1s infinite;\n    }\n  }\n  @media screen and (max-width: 900px) {\n    transform: scale(0.48) translate(58%, 20%) !important;\n  }\n}\n.btn-box {\n  display: flex !important;\n  position: absolute !important;\n  bottom: 12px;\n  width: 100%;\n  justify-content: space-around;\n  .btn {\n    height: 50px;\n    cursor: pointer;\n  }\n}","../public/style/welcome.scss":".title {\n  font-size: 78px;\n  letter-spacing: 10px;\n}\n","../public/style/home.scss":"#current-data {\n  display: flex !important;\n  width: 90%;\n  height: 60px;\n  align-items: center;\n  justify-content: space-between;\n  margin: 8% 5% 0;\n  .percent {\n    width: 50%;\n    border: 5px solid $main-color;\n    height: 30px;\n    > div {\n      height: 100%;\n      background-image: url(\"../images/percent.png\");\n      position: relative;\n      &::after {\n        content: '';\n        display: block;\n        background-size: contain;\n        width: 40px;\n        height: 40px;\n        position: absolute;\n        right: 0;\n        top: -54px;\n      }\n    }\n  }\n  .previous,\n  .next {\n    font-size: 50px;\n    display: block;\n    background-size: contain;\n    width: 60px;\n    height: 60px;\n    &.disabled {\n      opacity: 0.3;\n      pointer-events: none;\n    }\n  }\n  .previous {\n    background-image: url(\"../images/left.png\");\n  }\n  .next {\n    content: '';\n    background-image: url(\"../images/right.png\");\n    transform: rotateX(180deg);\n  }\n  @each $type in $data-type {\n    &.#{$type} .percent > div::after {\n      background-image: url(\"../images/#{$type}.png\");\n    }\n  }\n  @media screen and (max-width: 900px) {\n    transform: scale(0.45) translate(60%, 200%) !important;\n    // .dialogue {\n    //   transform: scale(0.48) translate(58%, 20%) !important;\n    // }\n  }\n}"},"lineCount":null}},"error":null,"hash":"f87c3dfae07d41c64d319a324320325e","cacheData":{"env":{}}}